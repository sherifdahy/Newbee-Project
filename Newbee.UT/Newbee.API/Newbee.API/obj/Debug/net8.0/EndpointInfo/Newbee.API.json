{
  "openapi": "3.0.1",
  "info": {
    "title": "Newbee.API",
    "version": "1.0"
  },
  "paths": {
    "/api/PickupLocations": {
      "get": {
        "tags": [
          "PickupLocations"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/PickupLocations/{id}": {
      "get": {
        "tags": [
          "PickupLocations"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Pricing": {
      "post": {
        "tags": [
          "Pricing"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PricingRequestDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/PricingRequestDTO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/PricingRequestDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "PricingRequestDTO": {
        "required": [
          "dropOffCity",
          "pickupCity"
        ],
        "type": "object",
        "properties": {
          "cod": {
            "type": "number",
            "format": "double"
          },
          "dropOffCity": {
            "minLength": 1,
            "type": "string"
          },
          "pickupCity": {
            "minLength": 1,
            "type": "string"
          },
          "size": {
            "type": "string",
            "nullable": true
          },
          "type": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}