{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Newbee.CS": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/newbee.cs",
      "sourceRoot": "projects/newbee.cs/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/newbee.cs/src/main.ts",
            "tsConfig": "projects/newbee.cs/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/newbee.cs/public"
              }
            ],
            "styles": [
              "projects/newbee.cs/src/styles.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "Newbee.CS:build:production"
            },
            "development": {
              "buildTarget": "Newbee.CS:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/newbee.cs/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/newbee.cs/public"
              }
            ],
            "styles": [
              "projects/newbee.cs/src/styles.css"
            ]
          }
        }
      }
    },
    "Newbee.WS": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/newbee.ws",
      "sourceRoot": "projects/newbee.ws/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/newbee.ws/src/main.ts",
            "tsConfig": "projects/newbee.ws/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/newbee.ws/public"
              }
            ],
            "styles": [
              "projects/newbee.ws/src/styles.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "Newbee.WS:build:production"
            },
            "development": {
              "buildTarget": "Newbee.WS:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/newbee.ws/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/newbee.ws/public"
              }
            ],
            "styles": [
              "projects/newbee.ws/src/styles.css"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "190e5ca2-e21e-4ea0-8289-a421101102d9"
  }
}
